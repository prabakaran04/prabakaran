<odoo>
    <template id="web.external_layout_standard">
        <div class="header"
            style="background-image:url(/medicalmanagement/static/img/Green.jpg); height:120px;width:100%;">
            <div class="row">
                <div class="col-3">
                    <img src="/medicalmanagement/static/img/medical_log.png"  height="100%" width="100%" style = "margin-left:10px" />
                </div>
            </div>
        </div>

        <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name"
            t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
            <t t-call="web.address_layout" />
            <t t-raw="0" />
        </div>
        <div class="footer"
            style=" background-image:url(/medicalmanagement/static/img/Green.jpg); height:120px;width:100%;">
        </div>
    </template>
</odoo>